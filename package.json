{
  "name": "wayde-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "commit": "cz",
    "postinstall": "yarn git:include-shared-config; husky install && { yarn husky:cz; yarn husky:commitlint }",
    "git:include-shared-config": "git config --local include.path ../.gitconfig",
    "husky:cz": "husky set .husky/prepare-commit-msg 'if [ -z $COMMIT_SOURCE ]; then exec < /dev/tty && yarn cz --hook || false; fi'",
    "husky:commitlint": "husky set .husky/commit-msg 'yarn commitlint --edit $1'"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/testing-library__jest-dom": "^5.14.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@commitlint/cz-commitlint": "^17.4.2",
    "commitizen": "^4.3.0",
    "eslint": "^8.34.0",
    "husky": "^8.0.3",
    "inquirer": "8"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 3.3.1",
    "node": ">= 18.12.1"
  }
}
